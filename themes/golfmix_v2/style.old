	text-transform:none;
}
.searchform-controls {
clear:none !important;
}
.searchform-controls input, #bottom-row #searchform input[type="submit"] {
	float: right;
	cursor: pointer;
	position: relative;
	background: #ECECEC;
	padding: 10px 7px;
	border-radius: 11px;
	-moz-border-radius: 11px;
	-webkit-border-radius: 11px;
	border: 1px solid #777;
