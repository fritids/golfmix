/* Main CSS Start */
body {
    background: url("images/body-bg.gif") repeat scroll 0 0 transparent !important;
    color: #666666;
    font-family: Arial,Helvetica,sans-serif;
    font-size: 12px;
    padding: 0;
}
h1 {
	font-size:24px;
	color:#333;
	margin:10px auto;
	margin-left:4px;
}
h1.page-title {
	margin:10px auto;
	margin-left:4px;
}
h2 {
	font-size:15px;
	color:#00507C;
	padding:0 0 7px 0;
}
h3 {
	font-size:16px;
	color:#2c2b2b;
	font-family:Arial, Helvetica, sans-serif;
	text-transform:uppercase;
	padding:0;
}
h4 {
	font-size:13px;
	color:#26558B;
	padding:0 0 10px 0;
}
h5 {
	font-size:12px;
	color:#26558B;
	padding:0 0 10px 0;
}
h6 {
	font-size:12px;
	color:#8a4355;
	padding:0 0 10px 0;
}
ul {
	padding:0 0 10px 30px;
	list-style:disc;
}
ul li {
}
ol {
	padding:0 0 10px 30px;
	list-style:decimal;
}
ol li {
}
a {
	color:#3096b8;
	text-decoration:underline;
}
a:hover {
	text-decoration:none;
}
p {
	padding:0 0 15px 0;
	color:#636363;
}
img.centered {
	display: block;
	margin-left: auto;
	margin-right: auto;
	margin-bottom:23px;
}
img.alignright {
	padding: 0px;
	margin:0 0 10px 10px;
	display: inline;
}
img.kjgrcGravatar {
	padding: 0px;
	margin:5px 10px 5px 0;
	display: inline;
}
img.aligncenter {
	display: block;
	padding: 0px;
	margin:0 10px 0 10px;
}
.alignright {
	float: right;
}
.kjgrcGravatar {
	float: left;
}
#wrapper {
	width: 1095px;
	background:url(images/wrapper-bg.png) repeat-y center;
	margin: 0 auto;
}
#inner-wrapper {
	width: 1080px;
	padding-bottom: 12px;
	margin: 0 auto;
}
/* Main CSS End */


/* Header CSS Start */
#header {
	width: 1088px;
	background: white;
	height: 118px;
}
.logo {
	width:127px;
	height:71px;
	float:left;
	padding:24px 0 0 22px;
}
.login-box {
	width: 217px;
	float:right;
	padding:0 6px 0 0;
	margin-top:50px;
}
.login-box ul {
	list-style:none;
	padding:0;
}
.login-box ul li {
	float:left;
	padding:0 0 10px 0;
}
.login-box ul li a {
	font-family:Arial, Helvetica, sans-serif;
	font-size:13px;
	color:#373636;
	text-decoration:none;
}
.login-box ul li a:hover {
	text-decoration:underline;
}
.login-box ul li a.twitter, .login-box ul li a.facebook, .login-box ul li a.rss, .login-box ul li a.appstore {
	display:block;
	width:31px;
	height:31px;
	text-indent:-9000px;
	padding:0 9px 0 0;
}
.login-box ul li a.appstore {
	width: 88px;
	clear:both;
	background:url(images/app-store.png) no-repeat;
	background-position:0 0;
}
.login-box ul li a.rss {
	padding:none!important;
}
.login-box ul li a.twitter {
	background:url(images/social-icons.jpg) no-repeat;
	background-position:0 0;
}
.login-box ul li a.facebook {
	background:url(images/social-icons.jpg) no-repeat;
	background-position:-40px 0;
}
.login-box ul li a.rss {
	background:url(images/social-icons.jpg) no-repeat;
	background-position:-80px 0;
}
/* Header CSS End */


/* Menu CSS Start */
#outer-menu-cont {
	width: 1083px;
	height:96px;
	background:none;
	margin:0 auto;
}
#menu-cont-left {
	width:17px;
	height:96px;
	float:left;
	background:url(images/menu-left.gif) no-repeat left;
	display: none;
}
#menu-cont-right {
	width:17px;
	height:96px;
	float:left;
	background:url(images/menu-right.gif) no-repeat right;
	display: none;
}
#menu-cont-center {
	width: 1049px;
	float: left;
	background: url(images/menu-center.gif) repeat-x;
	padding: 0 17px;
	border-radius: 12px;
	height: 96px;
}
#top-row {
	/*width:1158px;*/
	height:46px;
	float:left;
}
#menu {
	width: 916px;
	float:left;
	position:relative;
	z-index:10;
}
#menu ul {
	list-style:none;
	padding:0;
}
#menu ul li {
	float:left;
	padding:0 17px 0 17px;
	background:url(images/menu-seprator.gif) no-repeat right;
	position:relative;
}
#menu ul li:first-child {
	float:left;
	padding:0 17px 0 0;
	background:url(images/menu-seprator.gif) no-repeat right;
}
#menu ul li:last-child {
	background:none;
}
#menu ul li a {
	font-family:Arial, Helvetica, sans-serif;
	font-size:13px;
	color:#fcfbf8;
	line-height:46px;
	text-decoration:none;
}
#menu ul li a:hover {
	text-decoration:underline;
}
/************************  */
#menu ul li ul {
position:absolute;
left:0;
top:35px;
width:170px;
background:#252424;
padding:7px 0 7px 0;
}

#menu ul li ul li{
	float:left;
	padding:0 17px 0 17px;
	background:none;
	position:relative;
}
#menu ul li ul li:first-child {
	float:left;
	padding:0 17px 0 17px;
	background:none;
}

#menu ul li ul li a {
	font-family:Arial, Helvetica, sans-serif;
	font-size:13px;
	color:#fcfbf8;
	line-height:25px;
	text-decoration:none;
}

/************************  */
#bottom-row {
	/*width:1158px;*/
	height:50px;
	width:100%;
