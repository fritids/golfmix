}
.custom_search_custom_fields__search, #bottom-row #searchform {
	width:610px;
	float:left;
}
.search-bar {
	width:900px;
	float:left;
	position:relative;
}
.search-bar ul {
	list-style:none;
	padding:0;
}
.searchform-label {
	float: left !important;
	line-height: 42px;
	font-family: Arial, Helvetica, sans-serif !important;
	font-size: 13px !important;
	color: #FCFBF8;
	text-transform: uppercase;
	padding: 10px;
	position: relative;
	width: auto !important;
	font-weight: normal !important;
}
#bottom-row #searchform li {
float:left:
list-style: none;
display: inline;
}
.searchform-param span, #bottom-row #searchform span {
	width:284px;
	height:20px;
	padding:8px;
	float:left;
	margin-top:6px;
	background:url(images/search-box-text-box.png) no-repeat;
}
.searchform-param span input, #bottom-row #searchform span input {
	width:284px;
	float:left;
